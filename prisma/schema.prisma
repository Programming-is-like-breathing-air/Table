generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Task {
  id       String  @id @default(cuid())
  title    String  @db.VarChar(255)
  status   String  @db.VarChar(255)
  label    String?
  priority String  @db.VarChar(255)
}
